# CV Generation Configuration
# Set to 'true' to enable async CV generation mode
# When enabled, generateCV will use initiateCVGeneration instead of synchronous generation
VITE_ENABLE_ASYNC_CV_GENERATION=false

# CV Preview Configuration
# Set to 'true' to use backend generateCVPreview function for more accurate previews
# When disabled, uses local CVTemplateGenerator (faster but may not match final output)
VITE_USE_BACKEND_PREVIEW=false

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-auth-domain
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-storage-bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Stripe Configuration (Payment Processing)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key